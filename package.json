    "image-size": "2.0.2",
    "js-yaml": "^4.1.0",
    "kbar": "0.1.0-beta.45",
    "lucide-react": "^0.515.0",
    "mdast-util-to-string": "^4.0.0",
    "motion": "^12.23.24",
    "next": "15.4.7",
    "next-contentlayer2": "0.5.8",
    "next-themes": "^0.4.6",
    "novel": "^1.0.2",
    "postcss": "^8.5.5",
    "postgres": "^3.4.7",
    "probe-image-size": "^7.2.3",
